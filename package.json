{
  "name": "sam-cerebral",
  "version": "1.0.0",
  "main": "index.js",
  "author": "Slađan Ristić",
  "license": "MIT",
  "scripts": {
    "format": "prettier --write --trailing-comma all \"src/**/*.js\"",
    "lint": "eslint \"src/**/*.js\"",
    "prebuild": "mkdirp dist",
    "build": "npm run format && npm run lint && browserify src/main.js -d -o dist/bundle.js -t [babelify]",
    "start": "budo src/main.js:dist/bundle.js --pushstate --live -- -t babelify"
  },
  "devDependencies": {
    "babel-plugin-syntax-trailing-function-commas": "^6.22.0",
    "babel-plugin-transform-async-generator-functions": "^6.24.1",
    "babel-plugin-transform-async-to-generator": "^6.24.1",
    "babel-plugin-transform-es2015-modules-commonjs": "^6.24.1",
    "babel-plugin-transform-function-bind": "^6.22.0",
    "babel-plugin-transform-object-rest-spread": "^6.23.0",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-react": "^6.24.1",
    "babelify": "^7.3.0",
    "browserify": "^14.3.0",
    "budo": "^10.0.3",
    "eslint-plugin-react": "^7.0.0",
    "jsverify": "^0.8.2",
    "mkdirp": "^0.5.1",
    "tape": "^4.6.3"
  },
  "dependencies": {
    "cerebral": "next",
    "classnames": "^2.2.5",
    "color": "^1.0.3",
    "dynamics": "^1.0.0",
    "free-style": "^2.3.1",
    "postcss-js": "^0.3.0",
    "pull-stream": "^3.5.0",
    "ramda": "^0.23.0",
    "react": "^15.5.4",
    "react-dom": "^15.5.4",
    "rxjs": "^5.3.0",
    "setimmediate": "^1.0.5",
    "snabbt.js": "^0.6.4",
    "trine": "^0.1.1"
  }
}