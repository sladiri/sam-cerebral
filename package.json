{
  "name": "sam-cerebral",
  "version": "1.0.0",
  "main": "index.js",
  "author": "Slađan Ristić",
  "license": "MIT",
  "scripts": {
    "build-server": "mkdir -p dist-server && babel \"src/{server,universal}/**/*.js\" --out-dir dist-server",
    "build-browser": "mkdir -p static/dist && retry -- browserify src/browser/index.js -d -o static/dist/bundle.js -t [babelify]",
    "format": "prettier --write --trailing-comma all \"src/**/*.js\"",
    "prelint": "npm run format",
    "lint": "eslint \"src/**/*.js\"",
    "prebuild": "npm run lint",
    "build": "npm run build-server && npm run build-browser",
    "clean": "rm -rf static/dist && rm -rf dist-server",
    "start-budo": "budo src/browser/index.js:static/dist/bundle.js --host localhost --pushstate --live -- -t babelify",
    "startup-server-dev-watch": "nodemon --delay 100ms --watch src/server/ --watch src/universal/ server --exec \"babel-node src/server/index.js\"",
    "build-browser-watch": "nodemon --delay 100ms --watch src/browser/ --watch src/universal/ --exec npm run build-browser",
    "start-dev": "parallelshell \"npm run build-browser-watch\" \"npm run startup-server-dev-watch\"",
    "start": "node dist-server/src/server/index.js"
  },
  "devDependencies": {
    "babel-cli": "^6.24.1",
    "babel-eslint": "^7.2.3",
    "babel-plugin-syntax-trailing-function-commas": "^6.22.0",
    "babel-plugin-transform-async-generator-functions": "^6.24.1",
    "babel-plugin-transform-async-to-generator": "^6.24.1",
    "babel-plugin-transform-do-expressions": "^6.22.0",
    "babel-plugin-transform-es2015-modules-commonjs": "^6.24.1",
    "babel-plugin-transform-function-bind": "^6.22.0",
    "babel-plugin-transform-object-rest-spread": "^6.23.0",
    "babel-plugin-transform-react-jsx": "^6.24.1",
    "babel-polyfill": "^6.23.0",
    "babelify": "^7.3.0",
    "browserify": "^14.4.0",
    "budo": "^10.0.3",
    "eslint": "^4.1.1",
    "eslint-plugin-react": "^7.0.1",
    "jsverify": "^0.8.2",
    "nodemon": "^1.11.0",
    "parallelshell": "^3.0.1",
    "prettier": "^1.5.2",
    "retry-cli": "^0.4.0",
    "tape": "^4.7.0"
  },
  "dependencies": {
    "@cerebral/router": "0.0.0-beta.3",
    "asap": "^2.0.5",
    "cerebral": "^2.0.0-beta.3",
    "classnames": "^2.2.5",
    "color": "^1.0.3",
    "dynamics": "^1.0.0",
    "koa": "^2.3.0",
    "koa-static": "^3.0.0",
    "postcss-js": "^1.0.0",
    "pouchdb": "^6.2.0",
    "pouchdb-adapter-memory": "^6.2.0",
    "pull-stream": "^3.6.0",
    "ramda": "^0.24.1",
    "react": "^15.5.4",
    "react-dom": "^15.5.4",
    "react-free-style": "^4.3.2",
    "react-hyperscript": "^3.0.0",
    "rxjs": "^5.4.0",
    "setimmediate": "^1.0.5",
    "snabbt.js": "^0.6.4",
    "trine": "^0.1.1"
  }
}
